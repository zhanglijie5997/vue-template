{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "dev-inspect": "npx vue-cli-service inspect --mode development >> config/webpack.config.development.js",
    "pro-inspect": "npx vue-cli-service inspect --mode production >> config/webpack.config.production.js",
    "test:e2e": "vue-cli-service test:e2e",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "git":"git add . && git commit -m",
    "push":"git push"
  },
  "dependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "core-js": "^2.6.5",
    "register-service-worker": "^1.6.2",
    "vant": "^2.0.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/qs": "^6.5.3",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "amfe-flexible": "^2.2.1",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.1",
    "compression-webpack-plugin": "^3.0.0",
    "husky": "^3.0.5",
    "node-sass": "^4.12.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "1.18.2",
    "qs": "^6.8.3",
    "sass-loader": "^8.0.0",
    "ts-import-plugin": "^1.6.1",
    "typescript": "^3.4.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-cli-plugin-vant": "^1.0.1",
    "vue-template-compiler": "^2.6.10",
    "vuex-class": "^0.3.2"
  },
  "postcss": {
    "plugins": {
      "postcss-pxtorem": {
        "rootValue": 37.5,
        "propList": [
          "*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Android >= 4.0",
    "iOS >= 7"
  ]
}
